{
  "name": "webflow-extension-starter",
  "version": "1.0.0",
  "description": "Starter template for building Webflow Designer Extensions.",
  "keywords": [
    "designer-extension",
    "webflow"
  ],
  "main": "dist/index.js",
  "scripts": {
    "dev": "concurrently \"pnpm run watch\" \"webflow extension serve\"",
    "watch": "rspack --watch --mode development",
    "build": "rspack --mode production",
    "type-check": "tsc --noEmit",
    "check": "ultracite check --type-aware --type-check",
    "fix": "ultracite fix --type-aware --type-check",
    "bundle": "pnpm run build && webflow extension bundle",
    "serve": "webflow extension serve",
    "bump-deps": "npx npm-check-updates -u -p pnpm && pnpm install"
  },
  "dependencies": {
    "react": "^19.2.4",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@rspack/cli": "^1.7.0",
    "@rspack/core": "^1.7.0",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@webflow/designer-extension-typings": "^2.0.24",
    "concurrently": "^9.2.1",
    "oxfmt": "^0.21.0",
    "oxlint": "^1.36.0",
    "oxlint-tsgolint": "^0.10.1",
    "typescript": "^5.9.3",
    "ultracite": "7.0.7"
  },
  "packageManager": "pnpm@10.27.0+sha512.72d699da16b1179c14ba9e64dc71c9a40988cbdc65c264cb0e489db7de917f20dcf4d64d8723625f2969ba52d4b7e2a1170682d9ac2a5dcaeaab732b7e16f04a"
}
